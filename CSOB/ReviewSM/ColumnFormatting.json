{
    "$schema": "https://developer.microsoft.com/json-schemas/sp/v2/column-formatting.schema.json",
    "elmType": "div",
    "style": {
      "box-sizing": "border-box",
      "border-radius": "10px",
      "padding": "0 10px",
      "overflow": "hidden",
      "text-overflow": "ellipsis",
      "margin": "4px 4px 4px 4px",
      "font-weight": "bold"
    },
    "attributes": {
      "class": ""
    },
    "children": [
      {
        "elmType": "span",
        "style": {
          "line-height": "16px",
          "height": "14px",
          "display": "inherit"
        },
        "attributes": {
          "iconName": "People",
          "class": ""
        }
      },
      {
        "elmType": "span",
        "style": {
          "overflow": "hidden",
          "text-overflow": "ellipsis",
          "padding": "0 3px"
        },
        "txtContent": "[$Zpracovatel.title]",
        "attributes": {
          "class": ""
        },
        "defaultHoverField": "[$Zpracovatel]"
      }
    ]
  }